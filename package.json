  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BusanDevelopers/BGM-Event-Calendar-Webpage.git"
  },
  "bugs": {
